<!DOCTYPE html>
<html>
  <head>
    <%-include('./partials/head.ejs')%>
  <body>
    <%-include('./partials/navbar.ejs')%>
    <div class="table-responsive-sm ">
    <table id="ordersTable" class="table table-striped table-bordered align-middle text-center">
      <thead>
       <% if (user && user.username)  { %>
        <tr>
          <th scope ="col">Id</th>
          <th scope ="col">Name</th>
          <th scope ="col">Discount</th>
          <th scope ="col">Min</th>
          <th scope ="col">Max</th>
        </tr>
      </thead>
      <tbody id="memebershipTableContent">
        <% membership.forEach(function(item) {%>
              <tr>
                <td>
                  <%= item.id %>
                </td>
                <td>
                  <%= item.name  %>
                </td>
                <td>
                  <%= item.discount + '%' %> 
                </td>
                <td>
                  <%= item.min +' items' %>
                </td>
                <td>
                  <%= item.max + ' items' %>
                </td>
                <td>
                  <%= item.description  %>
                </td>
              </tr>
        <%});%>
      </tbody>
    </table>
    </div>
     <% } %>
     <%-include('./partials/footer.ejs')%> 
  </body>
</html>
